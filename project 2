#!/bin/Groovy
pipeline {
      agent any
      stages {
          stage( 'what is your name') {

              steps {
                  echo 'My name is Sly'

              }
           }
           stage( 'check date') {

              steps {
                  sh 'date'

              }
           }
           stage( 'check calendar') {

              steps {
                  sh 'date'

              }
           }
           stage( 'project') {

              steps {
                  echo 'software for health monitoring'

              }
           }
           hello world


<html>
  <head>
    <title>Body bgcolor Attribute example</title>
  </head>
  <body bgcolor="grey">
    <h1>This is sly webpage.</h1>
  </body>
</html>

